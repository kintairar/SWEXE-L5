<h1>ログイン</h1>
<%= form_with url: '/top/login', method: :post, local: true do |f| %>
<%= f.label :uid, 'ユーザーID' %>
<%= f.text_field :uid %>
<%= f.label :pass, 'パスワード' %>
<%= f.password_field :pass %>
<%= f.submit 'ログイン' %>
<% end %>
<%= link_to "ユーザ登録", new_user_path %>
<%= link_to 'ユーザ一覧', users_path %>